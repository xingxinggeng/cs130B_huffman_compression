#Makefile for cs130b project01

all: frequencies huffman encode decode

frequencies: frequencies.cpp
	g++ frequencies.cpp -o frequencies

huffman: huffman.cpp
	g++ huffman.cpp -o huffman

encode: encode.cpp
	g++ encode.cpp -o encode

decode: decode.cpp
	g++ decode.cpp -o decode

clean:
	rm -f frequencies huffman encode decode

